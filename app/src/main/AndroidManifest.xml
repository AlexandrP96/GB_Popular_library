<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ru.alexbox.gb_popular_l">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".lesson_6.dagger.app.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity android:name=".lesson_6.dagger.DaggerActivity" />
        <activity android:name=".lesson_6.di.DiActivity" />
        <activity android:name=".lesson_6.nodi.NoDiActivity" />
        <activity android:name=".lesson_6.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>